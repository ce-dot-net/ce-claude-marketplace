{
  "name": "ace-orchestration",
  "version": "3.3.0",
  "description": "ACE (Agentic Context Engineering) - Full ACE Paper Implementation with Semantic Search & Delta Operations. Three-Agent Architecture (Generator → Reflector → Curator). Learns from execution feedback. Based on Stanford/SambaNova/UC Berkeley research (arXiv:2510.04618v1)",
  "author": {
    "name": "ACE Team",
    "email": "ace@ce-dot-net.com"
  },
  "homepage": "https://github.com/ce-dot-net/ce-ai-ace",
  "repository": "https://github.com/ce-dot-net/ce-ai-ace",
  "license": "MIT",
  "keywords": [
    "claude-code",
    "ace",
    "pattern-learning",
    "self-improving",
    "context-engineering",
    "agentic",
    "research",
    "stanford",
    "autonomous"
  ],
  "hooks": "./hooks/hooks.json",
  "commands": "./commands",
  "agents": "./agents",
  "mcpServers": {
    "ace-pattern-learning": {
      "command": "npx",
      "args": ["-y", "@ce-dot-net/ace-client"],
      "env": {
        "ACE_SERVER_URL": "https://ace.your-domain.com",
        "ACE_API_TOKEN": "${ACE_API_TOKEN}",
        "ACE_PROJECT_ID": "${ACE_PROJECT_ID}"
      }
    }
  }
}
